<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd">

    <changeSet id="1" author="Arjun">
        <createTable tableName="test">
            <column name="id" type="int"></column>
            <column name="name" type="varchar(50)"></column>
        </createTable>
    </changeSet>
    <changeSet id="2" author="Arjun">
        <modifyDataType tableName="test" columnName="name" newDataType="varchar(45)"/>
    </changeSet>

	<changeSet author="liquibase-docs" id="3">
		<createProcedure >
			CREATE PROCEDURE InsertData()
			 BEGIN
			   INSERT INTO test (id, name) VALUES (1, 'Hello');
			 END;
		</createProcedure>
	</changeSet>
	<changeSet author="liquibase-docs" id="4">
		<sql>call InsertData</sql>
	</changeSet>

	<!-- <changeSet id="rollback" author="Arjun">
		<rollback>
			delete from test where id=1
		</rollback>
	</changeSet> -->
	
	<!-- <changeSet id="droptable" author="Arjun">
		<dropTable tableName="test"/>
	</changeSet> -->
	
</databaseChangeLog>